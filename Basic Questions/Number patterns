#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n; cin>>n;
    int r;
    for(int i=1 ; i<=n ; i++)
    {
        r = 2*i-1;
        for(int j=1 ; j<=n ; j++)
        {
            if(i==1 or i==n)    
            {
                cout<<r%10;
            }
            else if(j==1 or j==n)
            {
                    cout<<(r)%10;
            }    
            else cout<<" ";
        }
        cout<<endl;
    }
    return 0;
}
