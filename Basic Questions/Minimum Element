#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n; cin>>n;
    vector<int>arr(n);
    for(int i=0 ; i<n ; i++)    cin>>arr[i];
    //min
    int min = INT_MAX;
    for(int i=0 ; i<n ; i++)
    {
        if(arr[i]<min)
            min = arr[i];
    }
    cout<<"Minimum element = "<<min<<endl;
    return 0;
}
