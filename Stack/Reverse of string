#include<bits/stdc++.h>
using namespace std;
int main()
{
    string s = "Hello";
    string ans = "";
    stack<char>S;
    for(int i=0 ; i<s.size() ; i++)
    {
        S.push(s[i]);
    }
    for(int i=0 ; !S.empty(); i++)
    {
        ans+=S.top();
        S.pop();
    }
    cout<<ans<<endl;
    return 0;
}
